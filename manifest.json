{
   "background": {
      "scripts": [ "ajax_background.js" ]
   },
   "browser_action": {
      "default_icon": "icon48.png"
   },
   "content_scripts": [ {
      "js": [ "jquery-1.10.2.js", "ajax_content.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "description": "Record, replay, and convert ajax requests to JQuery in Google Chrome",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "manifest_version": 2,
   "name": "AJAX Sniffer",
   "permissions": [ "webRequest", "webRequestBlocking", "cookies", "tabs", "http://*/*", "https://*/*" ],
   "short_name": "AJAX Sniffer",
   "version": "1.0"
}
